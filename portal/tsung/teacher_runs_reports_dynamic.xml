<!-- load 3 random reports -->
<for from="0" to="2" incr="1" var="counter">
  <setdynvars sourcetype="eval"
    code="fun({Pid,DynVars})->
            Filterer=fun({Elem})-> re:run(Elem,'/portal/offerings/\d+/report$',[]), match end,
            {ok,Val}=ts_dynvars:lookup('reporturls',DynVars),
            {ok,List}=ts_foo:filter(Filterer,Val),
            lists:nth(random:uniform(length(List)),List) end.">
    <var name="report" />
  </setdynvars>
  <thinktime random="true" value="5"/>
  <request subst="true">
    <match do="dump" when="match">Report for:</match>
    <http url="%%_report%%" version="1.1" method="GET"/>
  </request>
